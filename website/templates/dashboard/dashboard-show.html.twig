{% extends 'base.html.twig' %}
{% import '@SpipuUi/macro/buttons.html.twig' as buttons %}

{% set menuCurrentItem="home" %}

{% block header_title %}{{ 'spipu.dashboard.page.home.title'|trans() }}{% endblock %}

{% block main_title %}{{ 'spipu.dashboard.page.home.title'|trans() }}{% endblock %}

{% block main_class %}container-fluid{% endblock %}

{% block stylesheets %}{{ parent() }}
    <link rel="stylesheet" href="{{ asset('/bundles/spipudashboard/css/dashboard.css') }}" >
{% endblock %}

{% block body %}
    {{ renderManager(manager) }}
{% endblock body %}

{% block javascripts %}{{ parent() }}
    <script src="{{ asset('/bundles/spipuui/js/google-graph-plots.js') }}"></script>
    <script src="{{ asset('/bundles/spipuui/js/spipu-graph-plots.js') }}"></script>
    <script src="{{ asset('/bundles/spipudashboard/js/dashboard-selector.js') }}"></script>
    <script src="{{ asset('/bundles/spipudashboard/js/widget-configuration.js') }}"></script>
    {% include manager.definition.templateShowJs %}
{% endblock javascripts %}